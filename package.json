{
  "name": "rivora-defi-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "production": "npm run clean && npm run lint && npm run type-check && npm run build",
    "collect-data": "ts-node --project tsconfig.scripts.json scripts/collect-training-data.ts",
    "train-model": "ts-node --project tsconfig.scripts.json scripts/train-ml-model.ts",
    "augment-data": "ts-node --project tsconfig.scripts.json scripts/augment-training-data.ts",
    "ml-setup": "npm run collect-data && npm run train-model",
    "ml-setup-1000": "npm run collect-data && npm run augment-data && mv training-data-augmented.json training-data.json && npm run train-model",
    "contract:build": "./scripts/build-contract.sh",
    "contract:deploy": "./scripts/deploy-contract.sh",
    "contract:test": "cd contracts/rivora-scores && cargo test"
  },
  "dependencies": {
    "@stellar/freighter-api": "^1.5.0",
    "@stellar/stellar-sdk": "^14.3.0",
    "@tanstack/react-query": "^5.84.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "lucide-react": "^0.344.0",
    "next": "^14.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "pino-pretty": "^13.1.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
